{
  "name": "immichframe",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.immichframe.immichframe",
    "productName": "ImmichFrame",
    "files": [
      "main.js",
      "index.html",
      "package.json",
      "node_modules/**/*"
    ],
    "mac": {
      "target": ["app"],
      "arch": ["x64", "arm64"],
      "icon": "assets/AppIcon.icns"
    },
    "win": {
      "target": ["portable"],
      "arch": ["x64", "arm64"],
      "icon": "assets/AppIcon.ico"
    },
    "linux": {
      "target": ["AppImage"],
      "arch": ["x64", "armv7l", "arm64"],
      "icon": "assets/AppIcon.png"
    }
  }
}