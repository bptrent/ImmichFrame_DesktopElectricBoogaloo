{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "immichframe",
  "version": "1.0.7",
  "identifier": "com.immichframe.immichframe",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "immichframe",
        "width": 800,
        "height": 600,
        "fullscreen": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "dmg",
      "appimage",
      "deb",
      "rpm",
      "msi"
    ],
    "icon": [
      "icons/AppIcon.png",
      "icons/AppIcon.icns",
      "icons/AppIcon.ico"
    ],
    "deb": {
      "depends": [
        "libjavascriptcoregtk-4.1-0",
        "libsoup-3.0-0",
        "libwebkit2gtk-4.1-0",
        "libayatana-appindicator3-1",
        "librsvg2-2"
      ]
    },
    "rpm": {
      "depends": [
        "javascriptcoregtk4",
        "libsoup3",
        "webkit2gtk4",
        "ayatana-appindicator3",
        "librsvg2"
      ]
    }
  },
  "plugins": {}
}
